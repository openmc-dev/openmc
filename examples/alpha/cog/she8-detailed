SHE-8

basic
  neutron centimeters urrpt freya

criticality
  npart=20000 nbatch=1000 sdt=0.0001 nfirst=50 norm=1.0 nsource=1 0 0 0

assign-mc
  alpha eigenvalue
  1 yellow 2 slate 3 lime 4 gray

mix nlib=ENDFB8R0 ptlib=PT.ENDFB8R0.ACE sablib=T.ENDFB8R0
  mat=1  bunches  $ fuel disk
    u235  8.4461-5
    u238  3.3671-4
    b10   2.9494-9
    b11   1.2823-8
    c12   9.4777-2
    o16   8.9479-4
    si    2.0248-7
    al    6.3228-8
    fe    6.1094-8
    cr    2.1873-8
    mn    2.0701-8
    ni    1.9371-8
    mo    2.3709-8
    w     3.0932-8
    cd    2.0237-10
    h     7.9655-5
  mat=2  bunches  $ graphite sleeve
    b10   1.6752-9
    b11   7.5165-9
    c12   8.0719-2
    o16   3.2888-5
    n     2.0439-6
    h     6.4677-5
  mat=3  bunches  $ absorber (boron)
    b10   8.0971-5
    b11   3.5203-4
    c12   8.7437-2
    o16   3.4997-5
    h     6.9989-5
  mat=4  a-f 2.7  $ aluminum
    al    1

geometry
  use unit 6 array -99  $ hex lattice

define unit 1  $ fuel element
  sector 1 fuel      -1  13 -14  or  -1  16 -17
  sector 2 graphite  -3  10 -11  or  -3  19 -20  $ graphite caps
  sector 2 graphite  -2  11 -12  or  -1  18 -19  $ graphite plugs
  sector 2 graphite  -1  14 -16                  $ graphite disks (inner)
  sector 2 graphite  -1  12 -13  or  -1  17 -18  $ graphite disks (outer)
  sector 2 graphite   2  -3  11 -19              $ graphite sleeve
  sector 2 graphite   4  -5  11 -19              $ matrix tube

define unit 2  $ replacement reflector element
  sector 2 graphite  -6  10 -20
  sector 2 graphite   4  -5  11 -19              $ matrix tube

define unit 3  $ permanent reflector element
  sector 2 graphite  -8  10 -20

define unit 4  $ control rod - black - 50/30
  sector 3 boron      21 -22  10 -20
  sector 4 aluminum   23 -24  10 -20
  sector 2 graphite   25 -26  11 -19             $ matrix tube

define unit 5  $ control rod - gray - 42-1
  sector 3 boron     -27  10 -20
  sector 4 aluminum   28 -29  10 -20
  sector 2 graphite   30 -31  11 -19             $ matrix tube

define unit 6
  {tri-x
    -71.5  121.48
   -208   -121.48
     71.5 -121.48
   -120 120
    101 102 103 104
   fill
